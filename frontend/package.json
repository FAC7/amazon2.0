{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Front-end repo of the Re-creation of Amazon by FAC7",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "webpack-dev-server --hot --inline --config devwebpack.config.js",
    "build": "webpack --progress --config productionwebpack.config.js",
    "test": "jest",
    "watch": "webpack --progress --config productionwebpack.config.js --watch",
    "postinstall": "mkdir -p ../.git/hooks && cp ../hooks/pre-commit ../.git/hooks && chmod -R 755 ../.git/hooks"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils"
    ],
    "preprocessorIgnorePatterns": [
      "/src/js/Header/NavBar/NavBar.css/"
    ]
  },
  "keywords": [],
  "author": "FAC7",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-jest": "^9.0.3",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "jest-cli": "^0.9.2",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-slick": "^0.11.1",
    "style-loader": "^0.13.0",
    "tape": "^4.5.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "cookieparser": "^0.1.0",
    "react-modal": "^0.6.1",
    "react-router": "^2.0.1"
  }
}
